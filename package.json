{
  "name": "manatuner-pro",
  "version": "2.0.0",
  "private": true,
  "description": "ManaTuner Pro - Advanced MTG Manabase Analyzer inspired by Charles Wickham's Project Manabase",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:component": "vitest run --config vitest.config.js",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:tabs": "playwright test tests/e2e/tabs/",
    "test:responsive": "playwright test tests/e2e/responsive/",
    "test:accessibility": "playwright test tests/e2e/accessibility/",
    "test:performance": "playwright test tests/e2e/performance/",
    "test:core-flows": "playwright test tests/e2e/core-flows/",
    "test:mtg-logic": "vitest run tests/mtg-specific/",
    "test:mana-calc": "vitest run tests/mtg-specific/mana-calculations/",
    "test:deck-parser": "vitest run tests/mtg-specific/deck-parsing/",
    "test:edge-cases": "vitest run tests/mtg-specific/card-types/",
    "test:competitive": "vitest run tests/fixtures/competitive-decklists.js",
    "test:formats": "vitest run tests/mtg-specific/formats/",
    "test:visual": "playwright test --grep '@visual'",
    "test:mobile": "playwright test --project='Mobile Chrome'",
    "test:firefox": "playwright test --project='firefox'",
    "test:webkit": "playwright test --project='webkit'",
    "test:chromium": "playwright test --project='chromium'",
    "test:quick": "npm run test:unit && playwright test tests/e2e/core-flows/main-user-flows.spec.js",
    "test:full": "npm run test:unit && npm run test:e2e && npm run test:accessibility",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "serve:functions": "cd functions && npm run serve",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "playwright:install": "npx playwright install --with-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.10",
    "@mui/x-charts": "^6.0.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@sentry/react": "^9.30.0",
    "@sentry/tracing": "^7.120.3",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.80.10",
    "@tanstack/react-query-devtools": "^5.80.10",
    "@types/react-transition-group": "^4.4.12",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "firebase": "^9.17.2",
    "framer-motion": "^12.18.1",
    "lodash": "^4.17.21",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-hotkeys-hook": "^4.3.7",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-virtualized-auto-sizer": "^1.0.15",
    "react-window": "^1.8.8",
    "recharts": "^2.15.3",
    "redux-persist": "^6.0.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/ui": "^0.29.2",
    "c8": "^7.13.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "firebase-tools": "^14.19.1",
    "jest": "^30.0.1",
    "jsdom": "^26.1.0",
    "ts-jest": "^29.4.0",
    "typescript": "^4.9.3",
    "vercel": "^43.2.0",
    "vite": "^4.1.0",
    "vite-plugin-pwa": "^0.14.4",
    "vitest": "^0.29.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/project-manabase/manabase-analyzer.git"
  },
  "keywords": [
    "mtg",
    "magic",
    "manabase",
    "analysis",
    "probability",
    "deck"
  ],
  "author": "ManaTuner Pro Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
